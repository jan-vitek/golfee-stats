.page-header
  %h1=t '.title', :default => 'New Round'
= form_for :files, :url => {:action => 'upload'}, :html => { :class => 'form-horizontal' } do |f|
  .control-group
    = f.label :file1, 'Tips', :class => 'control-label'
    .controls
      = f.file_field :file1, :class => 'file_field'
  .control-group
    = f.label :file2, 'Points', :class => 'control-label'
    .controls
      = f.file_field :file2, :class => 'file_field'
  .form-actions
    = f.submit nil, :class => 'btn btn-primary'
    = link_to t('.cancel', :default => t("helpers.links.cancel")), root_path, :class => 'btn'
